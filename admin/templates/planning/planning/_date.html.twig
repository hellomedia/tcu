<div class="date" id="date-{{ date.id }}">
    <div class="group-header">
        <h4>{{ date }}</h4>
    </div>
	<div class="courts">
		{% for court in courts %}
			<div class="court">
				<h5>{{ court }}</h5>
				<div class="slots">
					{% for slot in date.slotsByCourt(court) %}
                        {% include "@admin/planning/planning/_slot_row.html.twig" %}
					{% endfor %}
				</div>
			</div>
		{% endfor %}
	</div>
</div>
